<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>

    <add key="gitOrgName" value=":targetOrg"/>
    <add key="gitLogin" value=":yourGithubLogin"/>
    <add key="gitPassword" value=":youGithubPassword"/>

    <add key="formEmailSubject" value="Regarding your GitHub account" />
    <add key="formEmailMessage" value="Hi, {0}! You are part of my Github organization. I noticed you haven't added your name to your Github profile. Please go to www.github.com to update your profile. You can view your profile at {1}. Thanks for helping keep your profile up to date!" />

    <add key="awsAccessKey" value=":yourAwsAccessKey"/>
    <add key="awsSecretKey" value=":yourAwsSecretKey_REMOVE_SPACES"/>
    <add key="awsEndpoint" value="US-East-1" />
    <add key="awsBucket" value=":yourAwsBucket"/>
    <add key="awsLogName" value="NoNameUserLog"/>

  </appSettings>
  <system.net>
    <mailSettings>
      <smtp from=":yourEmail@gmail.com">
        <network
          host="smtp.gmail.com"
          port="587"
          userName=":yourGmailUserName"
          password=":yourGmailPassword"
          enableSsl="true"
        />
      </smtp>
    </mailSettings>
  </system.net>
</configuration>